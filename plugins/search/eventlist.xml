	<copyright>(C) 2005 - 2009 Christoph Lukes</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>info@schlu.net</authorEmail>
	<authorUrl>www.schlu.net</authorUrl>
	<version>1.0</version>
	<description>Search Plugin for the EventList Component</description>

	<files>
		<filename plugin="eventlist">eventlist.php</filename>
